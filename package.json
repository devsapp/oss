{
  "name": "oss",
  "version": "0.0.1",
  "main": "./lib/index.js",
  "dependencies": {
    "@alicloud/openapi-client": "^0.3.3"
  },
  "devDependencies": {
    "@serverless-devs/core": "^0.1.14",
    "@types/ali-oss": "^6.0.7",
    "@types/eslint": "^7.2.6",
    "@types/express": "^4.17.11",
    "@types/fs-extra": "^9.0.8",
    "@types/jest": "^26.0.10",
    "@types/lodash": "^4.14.170",
    "@types/node": "14",
    "@vercel/ncc": "^0.29.2",
    "ali-oss": "^6.13.2",
    "chillout": "^5.0.0",
    "f2elint": "^0.4.4",
    "fs-extra": "^9.1.0",
    "jest": "^26.4.0",
    "lodash": "*",
    "path-to-regexp": "^6.2.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.2.0",
    "ts-loader": "^8.0.14",
    "ts-node": "^9.1.1",
    "typescript": "^3.9.7",
    "walk-sync": "^2.2.0"
  },
  "scripts": {
    "start": "rimraf lib && npx tsc -w -p tsconfig.json",
    "prebuild": "rm -rf lib",
    "build": "ncc build src/index.ts -m -e @serverless-devs/core -e @alicloud/openapi-client -o lib",
    "test": "jest",
    "test:cov": "jest --coverage",
    "lint": "f2elint scan",
    "fix": "f2elint fix",
    "typecheck": "npx tsc -p tsconfig.json --noEmit",
    "update": "rm -rf package-lock.json && rm -rf node_modules && npm install"
  },
  "lint-staged": {
    "src/*.{js,jsx,ts}": "f2elint exec eslint"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  }
}
